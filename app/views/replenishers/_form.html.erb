<%= form_for @replenisher do |form| %>
  <div class="form-group">
    <%= form.label :fname %>
    <%= form.text_field :fname, id: :replenisher_fname, class: "form-control"  %>
  </div>

  <div class="form-group">
    <%= form.label :lname %>
    <%= form.text_field :lname, id: :replenisher_lname, class: "form-control"  %>
  </div>

  <div class="form-group">
    <%= form.label :email %>
    <%= form.text_field :email, id: :replenisher_email, class: "form-control"  %>
  </div>

  <div class="form-group">
    <%= form.label :phone %>
    <%= form.text_field :phone, id: :replenisher_phone, class: "form-control"  %>
  </div>

  <div class="form-group">
    <%= form.label :dni %>
    <%= form.text_field :dni, id: :replenisher_dni, class: "form-control"  %>
  </div>

  <div class="actions">
    <%= form.submit :class=>"btn btn-primary"%>
  </div>
<% end %>
